<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    tools:context=".assignment5.WordGame">

    <neu.edu.madcourse.dharaksavalia.numad17s_dharaksavalia.assignment5.ScrollingView
        android:id="@+id/wordmain_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:scrollingDrawable="@drawable/xando_background"/>

    <fragment
        android:id="@+id/wordmain_fragment"
        class="neu.edu.madcourse.dharaksavalia.numad17s_dharaksavalia.assignment5.MainFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        tools:layout="@layout/wordfragment_main"/>
</FrameLayout>
